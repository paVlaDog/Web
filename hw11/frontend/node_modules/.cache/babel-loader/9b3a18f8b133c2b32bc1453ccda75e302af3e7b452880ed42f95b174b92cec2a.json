{"ast":null,"code":"export default {\n  name: \"TicTacToeForm\",\n  data: function () {\n    return {\n      size: \"\",\n      needToWin: \"\",\n      error: \"\"\n    };\n  },\n  methods: {\n    createBoard: function () {\n      this.error = \"\";\n      this.$root.$emit(\"createBoard\", this.size, this.needToWin);\n    }\n  },\n  beforeCreate() {\n    this.$root.$on(\"onTicTacToeValidationError\", error => this.error = error);\n  }\n};","map":{"version":3,"mappings":"AAiCA;EACAA;EACAC;IACA;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACAC;MACA;MACA;IACA;EACA;EACAC;IACA;EACA;AACA","names":["name","data","size","needToWin","error","methods","createBoard","beforeCreate"],"sourceRoot":"src/components/TicTacToe","sources":["TicTacToeForm.vue"],"sourcesContent":["<template>\r\n  <div class=\"enter form-box\">\r\n    <div class=\"header\">New Game</div>\r\n    <div class=\"body\">\r\n      <form @submit.prevent=\"createBoard\">\r\n        <input type=\"hidden\" name=\"action\" value=\"enter\"/>\r\n        <div class=\"field\">\r\n          <div class=\"name\">\r\n            <label for=\"size\">Size:</label>\r\n          </div>\r\n          <div class=\"value\">\r\n            <input id=\"size\" name=\"size\" v-model=\"size\"/>\r\n          </div>\r\n        </div>\r\n        <div class=\"field\">\r\n          <div class=\"name\">\r\n            <label for=\"needToWin\">Need to win:</label>\r\n          </div>\r\n          <div class=\"value\">\r\n            <input id=\"needToWin\" name=\"needToWin\" v-model=\"needToWin\"/>\r\n          </div>\r\n        </div>\r\n        <div class=\"error\">{{ error }}</div>\r\n        <div class=\"button-field\">\r\n          <input type=\"submit\" value=\"Enter\">\r\n        </div>\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\n  export default {\r\n    name: \"TicTacToeForm\",\r\n    data: function () {\r\n      return {\r\n        size: \"\",\r\n        needToWin: \"\",\r\n        error: \"\"\r\n      }\r\n    },\r\n    methods: {\r\n      createBoard: function () {\r\n        this.error = \"\";\r\n        this.$root.$emit(\"createBoard\", this.size, this.needToWin);\r\n      }\r\n    },\r\n    beforeCreate() {\r\n      this.$root.$on(\"onTicTacToeValidationError\", error => this.error = error)\r\n    },\r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}